CREATE MATERIALIZED VIEW mat_m1(a, countb, sumbc, spreadcb, avgc)
WITH (timescaledb.continuous)
AS
SELECT time_bucket(1, a), count(*), sum(b+c), max(c)-min(b), avg(c)::int
FROM ht_intdata
WHERE b < 16
GROUP BY time_bucket(1, a)
HAVING sum(c) > 50 WITH NO DATA;
