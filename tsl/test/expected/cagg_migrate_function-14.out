  SELECT time, sensor_id, sensor_id * sensor_id + 100
    FROM generate_series('2020-01-01 0:00:00+0'::timestamptz, '2020-01-01 23:59:59+0','1m') g1(time),
    generate_series(1, 2, 1) AS g2(sensor_id);
