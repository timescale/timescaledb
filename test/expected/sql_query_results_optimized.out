--------------------------------------------------------------------
 Limit
   ->  Merge Append
         Sort Key: _hyper_1_0_replica."time" DESC
         ->  Sort
               Sort Key: _hyper_1_0_replica."time" DESC
               ->  Seq Scan on _hyper_1_0_replica
         ->  Sort
               Sort Key: _hyper_1_1_0_partition."time" DESC
               ->  Seq Scan on _hyper_1_1_0_partition
         ->  Index Scan using "1-time_plain" on _hyper_1_1_0_1_data
