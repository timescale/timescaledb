CREATE MATERIALIZED VIEW metrics_hourly
WITH (timescaledb.continuous) AS
SELECT time_bucket(INTERVAL '1 hour', time) AS bucket,
   AVG(value),
   MAX(value),
   MIN(value)
FROM metrics
GROUP BY bucket
WITH NO DATA;
